<entity-facade-xml type="seed">
    <!--  - mantle.work.time-->

    <!-- Timesheet Status -->
    <StatusType description="Timesheet" statusTypeId="Timesheet"/>
    <StatusItem description="In-Process" sequenceNum="1" statusId="TIMESHEET_IN_PROCESS" statusTypeId="Timesheet"/>
    <StatusItem description="Completed" sequenceNum="2" statusId="TIMESHEET_COMPLETED" statusTypeId="Timesheet"/>
    <StatusItem description="Approved" sequenceNum="3" statusId="TIMESHEET_APPROVED" statusTypeId="Timesheet"/>
    <StatusValidChange statusId="TIMESHEET_IN_PROCESS" statusIdTo="TIMESHEET_COMPLETED" transitionName="Confirm complete"/>
    <StatusValidChange statusId="TIMESHEET_COMPLETED" statusIdTo="TIMESHEET_APPROVED" transitionName="Approve"/>

    <!--  - mantle.work.effort -->

    <!-- Work Effort Type -->
    <EnumerationType description="Work Effort Type" enumTypeId="WorkEffortType"/>
    <Enumeration description="Task" enumId="WET_TASK" enumTypeId="WorkEffortType"/>
    <Enumeration description="Event" enumId="WET_EVENT" enumTypeId="WorkEffortType"/>
    <Enumeration description="Available" enumId="WET_AVAILABLE" enumTypeId="WorkEffortType"/>
    <Enumeration description="Time Off" enumId="WET_TIME_OFF" enumTypeId="WorkEffortType"/>

    <!-- Work Effort Purpose -->
    <EnumerationType description="Work Effort Purpose" enumTypeId="WorkEffortPurpose"/>

    <Enumeration description="Project" enumId="WEP_PROJECT" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Phase" enumId="WEP_PHASE" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Task" enumId="WEP_TASK" enumTypeId="WorkEffortPurpose"/>

    <Enumeration description="Manufacturing" enumId="ROU_MANUFACTURING" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Assembling" enumId="ROU_ASSEMBLING" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Routing" enumId="WEP_ROUTING" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Routing Task" enumId="WEP_ROUTING_TASK" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Production Run" enumId="WEP_PRODUCTION_RUN" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Fixed Asset Usage (rental)" enumId="WEP_ASSET_USAGE" enumTypeId="WorkEffortPurpose"/>

    <Enumeration description="Business Travel" enumId="WEP_BUSINESS_TRAVEL" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Meeting" enumId="WEP_MEETING" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Training" enumId="WEP_TRAINING" enumTypeId="WorkEffortPurpose"/>

    <Enumeration description="Maintenance" enumId="WEP_MAINTENANCE" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Research" enumId="WEP_RESEARCH" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Meeting" enumId="WEP_MEETING" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Development" enumId="WEP_DEVELOPMENT" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Support" enumId="WEP_SUPPORT" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Deployment" enumId="WEP_DEPLOYMENT" enumTypeId="WorkEffortPurpose"/>

    <Enumeration description="Sub-contracting" enumId="ROU_SUBCONTRACTING" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Phone Call" enumId="WEP_PHONE_CALL" enumTypeId="WorkEffortPurpose"/>
    <Enumeration description="Email" enumId="WEP_EMAIL" enumTypeId="WorkEffortPurpose"/>

    <!-- Work Effort Status -->
    <StatusType description="Work Effort Status" statusTypeId="WorkEffort"/>
    <StatusItem description="In Planning" sequenceNum="1" statusId="WE_IN_PLANNING" statusTypeId="WorkEffort"/>
    <StatusItem description="Scheduled" sequenceNum="2" statusId="WE_SCHEDULED" statusTypeId="WorkEffort"/>
    <StatusItem description="In Progress" sequenceNum="3" statusId="WE_IN_PROGRESS" statusTypeId="WorkEffort"/>
    <StatusItem description="Complete" sequenceNum="9" statusId="WE_COMPLETE" statusTypeId="WorkEffort"/>
    <StatusItem description="Closed" sequenceNum="10" statusId="WE_CLOSED" statusTypeId="WorkEffort"/>
    <StatusItem description="On Hold" sequenceNum="98" statusId="WE_ON_HOLD" statusTypeId="WorkEffort"/>
    <StatusItem description="Cancelled" sequenceNum="99" statusId="WE_CANCELLED" statusTypeId="WorkEffort"/>
    <StatusValidChange statusId="WE_IN_PLANNING" statusIdTo="WE_SCHEDULED" transitionName="Schedule"/>
    <StatusValidChange statusId="WE_IN_PLANNING" statusIdTo="WE_IN_PROGRESS" transitionName="Begin"/>
    <StatusValidChange statusId="WE_SCHEDULED" statusIdTo="WE_IN_PROGRESS" transitionName="Begin"/>
    <StatusValidChange statusId="WE_IN_PROGRESS" statusIdTo="WE_ON_HOLD" transitionName="Hold"/>
    <StatusValidChange statusId="WE_IN_PROGRESS" statusIdTo="WE_COMPLETE" transitionName="Complete"/>
    <StatusValidChange statusId="WE_COMPLETE" statusIdTo="WE_CLOSED" transitionName="Close"/>
    <StatusValidChange statusId="WE_ON_HOLD" statusIdTo="WE_IN_PROGRESS" transitionName="Resume"/>
    <StatusValidChange statusId="WE_IN_PROGRESS" statusIdTo="WE_CANCELLED" transitionName="Cancel"/>
    <StatusValidChange statusId="WE_ON_HOLD" statusIdTo="WE_CANCELLED" transitionName="Cancel"/>

    <!-- Work Effort Visibility -->
    <EnumerationType description="Work Effort Visibility" enumTypeId="WorkEffortVisibility"/>
    <Enumeration description="General (public access)" enumId="WES_PUBLIC" enumTypeId="WorkEffortVisibility"/>
    <Enumeration description="Restricted (private access)" enumId="WES_PRIVATE" enumTypeId="WorkEffortVisibility"/>
    <Enumeration description="Top Secret (confidential access)" enumId="WES_CONFIDENTIAL" enumTypeId="WorkEffortVisibility"/>

    <!-- Work Effort Asset Assignment Status -->
    <StatusType description="Work Effort Asset Assignment Status" statusTypeId="WorkEffortAssetAssign"/>
    <StatusItem description="Requested" sequenceNum="1" statusId="FA_ASGN_REQUESTED" statusTypeId="WorkEffortAssetAssign"/>
    <StatusItem description="Assigned" sequenceNum="2" statusId="FA_ASGN_ASSIGNED" statusTypeId="WorkEffortAssetAssign"/>
    <StatusItem description="Request Denied" sequenceNum="3" statusId="FA_ASGN_DENIED" statusTypeId="WorkEffortAssetAssign"/>

    <!-- Work Effort Association Type -->
    <EnumerationType description="Work Effort Association Type" enumTypeId="WorkEffortAssocType"/>
    <Enumeration description="Dependent" workEffortAssocTypeId="WEAT_DEPENDENT" enumTypeId="WorkEffortAssocType"/>
    <Enumeration description="Concurrent" workEffortAssocTypeId="WEAT_CONCURRENCY" enumTypeId="WorkEffortAssocType"/>
    <Enumeration description="Precedent" workEffortAssocTypeId="WEAT_PRECEDENCY" enumTypeId="WorkEffortAssocType"/>
    <Enumeration description="Clone" workEffortAssocTypeId="WEAT_CLONE" enumTypeId="WorkEffortAssocType"/>
    <Enumeration description="Routing Component" workEffortAssocTypeId="WEAT_ROUTING_COMP" enumTypeId="WorkEffortAssocType"/>

    <!-- Work Effort Content Type -->
    <EnumerationType description="Work Effort Content Type" enumTypeId="WorkEffortContentType"/>
    <Enumeration enumId="PROJECT_UPDATE" description="Project Update" enumTypeId="WorkEffortContentType"/>
    <Enumeration enumId="DISCUSSION" description="Discussion" enumTypeId="WorkEffortContentType"/>
    <Enumeration enumId="REQUIREMENT" description="Requirement" enumTypeId="WorkEffortContentType"/>
    <Enumeration enumId="DESIGN" description="Design" enumTypeId="WorkEffortContentType"/>
    <Enumeration enumId="PROPOSAL_MEDIA" description="Proposal Media" enumTypeId="WorkEffortContentType"/>
    <Enumeration enumId="SUPPORTING_MEDIA" description="Supporting Media" enumTypeId="WorkEffortContentType"/>
    <Enumeration enumId="CREATED_MEDIA" description="Created Media" enumTypeId="WorkEffortContentType"/>

    <!-- Work Effort Party Status -->
    <StatusType description="Work Effort Party Status" statusTypeId="WorkEffortParty"/>
    <StatusItem description="Offered" sequenceNum="1" statusId="PRTYASGN_OFFERED" statusTypeId="WorkEffortParty"/>
    <StatusItem description="Assigned" sequenceNum="2" statusId="PRTYASGN_ASSIGNED" statusTypeId="WorkEffortParty"/>
    <StatusItem description="Declined" sequenceNum="3" statusId="PRTYASGN_DECLINED" statusTypeId="WorkEffortParty"/>
    <StatusItem description="Unassigned" sequenceNum="4" statusId="PRTYASGN_UNASSIGNED" statusTypeId="WorkEffortParty"/>

    <!-- Work Effort Party Availability -->
    <EnumerationType description="Work Effort Party Availability" enumTypeId="WorkEffortPartyAvailability"/>
    <Enumeration description="Available" enumId="WEPA_AV_AVAILABLE" enumTypeId="WorkEffortPartyAvailability"/>
    <Enumeration description="Busy" enumId="WEPA_AV_BUSY" enumTypeId="WorkEffortPartyAvailability"/>
    <Enumeration description="Away" enumId="WEPA_AV_AWAY" enumTypeId="WorkEffortPartyAvailability"/>

    <!-- Work Effort Party Delegate Reason -->
    <EnumerationType description="Work Effort Party Delegate Reason" enumTypeId="WorkEffortPartyDelegateReason"/>
    <Enumeration description="Need Support or Help" enumId="WEDR_NEED_HELP" enumTypeId="WorkEffortPartyDelegateReason"/>
    <Enumeration description="My Part Finished" enumId="WEDR_PART_FINISHED" enumTypeId="WorkEffortPartyDelegateReason"/>
    <Enumeration description="Completely Finished" enumId="WEDR_WHOLE_FINISHED" enumTypeId="WorkEffortPartyDelegateReason"/>

    <!-- Work Effort Party Expectation -->
    <EnumerationType description="Work Effort Party Expectation" enumTypeId="WorkEffortPartyExpectation"/>
    <Enumeration description="For Your Information" enumId="WEE_FYI" enumTypeId="WorkEffortPartyExpectation"/>
    <Enumeration description="Involvement Required" enumId="WEE_REQUIRE" enumTypeId="WorkEffortPartyExpectation"/>
    <Enumeration description="Involvement Requested" enumId="WEE_REQUEST" enumTypeId="WorkEffortPartyExpectation"/>
    <Enumeration description="Immediate Response Requested" enumId="WEE_IMMEDIATE" enumTypeId="WorkEffortPartyExpectation"/>

    <!-- Work Effort Product Type -->
    <EnumerationType description="Work Effort Product Type" enumTypeId="WorkEffortProductType"/>
    <Enumeration enumId="ROU_PROD_TEMPLATE" description="Product and Routing Association" enumTypeId="WorkEffortProductType"/>
    <Enumeration enumId="PRUN_PROD_DELIV" description="Production Run and Product to Deliver Association" enumTypeId="WorkEffortProductType"/>
    <Enumeration enumId="PRUNT_PROD_NEEDED" description="Production Run Task and Needed Product Association" enumTypeId="WorkEffortProductType"/>
    <Enumeration enumId="PRUNT_PROD_DELIV" description="Production Run Task and Deliverable Product Association" enumTypeId="WorkEffortProductType"/>
    <Enumeration enumId="GENERAL_SALES" description="Product to Represent General Sales of the WorkEffort" enumTypeId="WorkEffortProductType"/>

    <!--  Work Effort Product Status -->
    <StatusType description="Work Effort Product Status" statusTypeId="WorkEffortProduct"/>
    <StatusItem description="Created" sequenceNum="1" statusId="WEPAS_CREATED" statusTypeId="WorkEffortProduct"/>
    <StatusItem description="Completed" sequenceNum="2" statusId="WEPAS_COMPLETED" statusTypeId="WorkEffortProduct"/>
    <StatusItem description="Cancelled" sequenceNum="3" statusId="WEPAS_CANCELLED" statusTypeId="WorkEffortProduct"/>
</entity-facade-xml>
