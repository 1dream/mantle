    <!-- ========================================================= -->
    <!-- mantle.accounting.ledger.account -->
    <!-- ========================================================= -->

    <entity entity-name="GlAccount" package-name="mantle.accounting.ledger.account">
        <field name="glAccountId" type="id" is-pk="true"/>
        <field name="glAccountTypeEnumId" type="id"/>
        <field name="glAccountClassEnumId" type="id"/>
        <field name="glResourceTypeEnumId" type="id"/>
        <field name="glXbrlClassEnumId" type="id"/>
        <field name="parentGlAccountId" type="id"/>
        <field name="accountCode" type="text-medium"/>
        <field name="accountName" type="text-medium"/>
        <field name="description" type="text-long"/>
        <field name="productId" type="id"/>
        <field name="externalId" type="id">
            <description>id of the account in an external system where the accounts are imported/exported</description>
        </field>
        <field name="postedBalance" type="currency-amount"/>
        <relationship type="one" title="GlAccountType" related-entity-name="Enumeration">
            <key-map field-name="glAccountTypeEnumId"/>
        </relationship>
        <relationship type="one" title="GlAccountClass" related-entity-name="Enumeration">
            <key-map field-name="glAccountClassEnumId"/>
        </relationship>
        <relationship type="one" title="GlResourceType" related-entity-name="Enumeration">
            <key-map field-name="glResourceTypeEnumId"/>
        </relationship>
        <relationship type="one" title="GlXbrlClass" related-entity-name="Enumeration">
            <key-map field-name="glXbrlClassEnumId"/>
        </relationship>
        <relationship type="one" title="Parent" related-entity-name="GlAccount">
            <key-map field-name="parentGlAccountId"/>
        </relationship>
        <index name="GLACCT_UNQCD" unique="true"><index-field name="accountCode"/></index>
    </entity>

    <entity entity-name="GlAccountCategory" package-name="mantle.accounting.ledger.account">
        <field name="glAccountCategoryId" type="id" is-pk="true"/>
        <field name="glAccountCategoryTypeEnumId" type="id"/>
