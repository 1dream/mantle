        <field name="reconciledDate" type="date-time"/>
        <relationship type="one" related-entity-name="GlAccount"/>
        <relationship type="one" title="Organization" related-entity-name="Party">
            <key-map field-name="organizationPartyId"/>
        </relationship>
        <relationship type="one" related-entity-name="StatusItem"/>
    </entity>
    <entity entity-name="GlReconciliationEntry" package-name="mantle.accounting.ledger.reconciliation">
        <field name="glReconciliationId" type="id" is-pk="true"/>
        <field name="acctgTransId" type="id" is-pk="true"/>
        <field name="acctgTransEntrySeqId" type="id" is-pk="true"/>
        <field name="reconciledAmount" type="currency-amount"/>
        <relationship type="one" related-entity-name="GlReconciliation"/>
        <relationship type="one-nofk" related-entity-name="AcctgTrans"/>
        <relationship type="one" related-entity-name="AcctgTransEntry">
            <key-map field-name="acctgTransId"/>
            <key-map field-name="acctgTransEntrySeqId"/>
        </relationship>
    </entity>

