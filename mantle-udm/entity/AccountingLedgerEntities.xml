        <relationship type="one" title="GlAccountCategoryType" related-entity-name="Enumeration">
            <key-map field-name="glAccountCategoryTypeEnumId"/>
        </relationship>
    </entity>
    <entity entity-name="GlAccountCategoryMember" package-name="mantle.accounting.ledger.account">
        <field name="glAccountId" type="id" is-pk="true"/>
        <field name="glAccountCategoryId" type="id" is-pk="true"/>
        <field name="fromDate" type="date-time" is-pk="true"/>
        <field name="thruDate" type="date-time"/>
        <field name="amountPercentage" type="number-decimal"/>
        <relationship type="one" related-entity-name="GlAccount"/>
        <relationship type="one" related-entity-name="GlAccountCategory"/>
    </entity>
    <entity entity-name="GlAccountGroup" package-name="mantle.accounting.ledger.account">
        <field name="glAccountGroupId" type="id" is-pk="true"/>
        <field name="glAccountGroupTypeEnumId" type="id"/>
        <field name="description" type="text-long"/>
        <relationship type="one" title="GlAccountGroupType" related-entity-name="Enumeration">
            <key-map field-name="glAccountGroupTypeEnumId"/>
        </relationship>
    </entity>
    <entity entity-name="GlAccountGroupMember" package-name="mantle.accounting.ledger.account">
        <field name="glAccountId" type="id" is-pk="true"/>
        <field name="glAccountGroupTypeEnumId" type="id" is-pk="true"/>
        <field name="glAccountGroupId" type="id"/>
        <relationship type="one" related-entity-name="GlAccount"/>
        <relationship type="one" title="GlAccountGroupType" related-entity-name="Enumeration">
            <key-map field-name="glAccountGroupTypeEnumId"/>
        </relationship>
        <relationship type="one" related-entity-name="GlAccountGroup"/>
    </entity>

    <entity entity-name="GlAccountHistory" package-name="mantle.accounting.ledger.account">
        <field name="glAccountId" type="id" is-pk="true"/>
        <field name="organizationPartyId" type="id" is-pk="true"/>
        <field name="timePeriodId" type="id" is-pk="true"/>
        <field name="postedDebits" type="currency-amount"/>
        <field name="postedCredits" type="currency-amount"/>
        <field name="endingBalance" type="currency-amount"/>
