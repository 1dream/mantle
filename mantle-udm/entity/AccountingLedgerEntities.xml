    <entity entity-name="TaxAuthorityGlAccount" package-name="mantle.accounting.ledger.config">
        <field name="taxAuthGeoId" type="id" is-pk="true"/>
        <field name="taxAuthPartyId" type="id" is-pk="true"/>
