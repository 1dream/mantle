<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-1.0.xsd">
    <!-- ========================================================= -->
    <!--  - mantle.party.agreement -->
    <!--  - mantle.party.communication -->
    <!--  - mantle.party.contact -->
    <!--  - mantle.party.party -->
    <!--  - mantle.party.time -->
    <!-- ========================================================= -->


    <!-- ========================================================= -->
    <!-- mantle.party.agreement -->
    <!-- ========================================================= -->

    <entity entity-name="Agreement" package-name="mantle.party.agreement">
        <field name="agreementId" type="id" is-pk="true"/>
        <field name="agreementTypeEnumId" type="id"/>
        <field name="agreementDate" type="date-time"/>
        <field name="fromPartyId" type="id"/>
        <field name="fromRoleTypeId" type="id"/>
        <field name="toPartyId" type="id"/>
        <field name="toRoleTypeId" type="id"/>
        <field name="fromDate" type="date-time"/>
        <field name="thruDate" type="date-time"/>
        <field name="description" type="text-long"/>
        <field name="textData" type="text-very-long"/>
        <relationship type="one" related-entity-name="Product"/>
        <relationship type="one" title="From" related-entity-name="Party">
            <key-map field-name="fromPartyId"/>
        </relationship>
        <relationship type="one" title="From" related-entity-name="RoleType">
            <key-map field-name="fromRoleTypeId"/>
        </relationship>
        <relationship type="one" title="To" related-entity-name="Party">
            <key-map field-name="toPartyId"/>
        </relationship>
        <relationship type="one" title="To" related-entity-name="RoleType">
            <key-map field-name="toRoleTypeId"/>
        </relationship>
        <relationship type="one" title="AgreementType" related-entity-name="Enumeration">
            <key-map field-name="agreementTypeEnumId"/>
        </relationship>
    </entity>
    <entity entity-name="AgreementAddendum" package-name="mantle.party.agreement">
        <field name="agreementAddendumId" type="id" is-pk="true"/>
        <field name="agreementId" type="id"/>
        <field name="agreementItemSeqId" type="id"/>
        <field name="addendumCreationDate" type="date-time"/>
        <field name="addendumEffectiveDate" type="date-time"/>
        <field name="addendumText" type="text-long"/>
        <relationship type="one" related-entity-name="Agreement"/>
        <relationship type="one" related-entity-name="AgreementItem">
            <key-map field-name="agreementId"/>
            <key-map field-name="agreementItemSeqId"/>
        </relationship>
    </entity>
    <entity entity-name="AgreementParty" package-name="mantle.party.agreement">
        <field name="agreementId" type="id" is-pk="true"/>
        <field name="partyId" type="id" is-pk="true"/>
        <field name="roleTypeId" type="id" is-pk="true"/>
        <relationship type="one" related-entity-name="Agreement"/>
        <relationship type="one" related-entity-name="Party"/>
        <relationship type="one" related-entity-name="RoleType"/>
    </entity>
    <entity entity-name="AgreementTerm" package-name="mantle.party.agreement">
        <field name="agreementTermId" type="id" is-pk="true"/>
        <field name="agreementId" type="id"/>
        <field name="agreementItemSeqId" type="id"/>
        <field name="settlementTermId" type="id"/>
        <field name="termTypeEnumId" type="id"/>
        <field name="fromDate" type="date-time"/>
        <field name="thruDate" type="date-time"/>
        <relationship type="one" related-entity-name="Agreement"/>
        <relationship type="one-nofk" related-entity-name="AgreementItem">
            <key-map field-name="agreementId"/>
            <key-map field-name="agreementItemSeqId"/>
        </relationship>
        <relationship type="one" related-entity-name="SettlementTerm"/>
        <relationship type="one" title="TermType" related-entity-name="Enumeration">
            <key-map field-name="itemTypeEnumId"/>
        </relationship>
    </entity>

    <entity entity-name="AgreementItem" package-name="mantle.party.agreement">
        <field name="agreementId" type="id" is-pk="true"/>
        <field name="agreementItemSeqId" type="id" is-pk="true"/>
        <field name="agreementItemTypeEnumId" type="id"/>
        <field name="currencyUomId" type="id"/>
        <field name="fromDate" type="date-time"/>
        <field name="thruDate" type="date-time"/>
        <field name="itemText" type="text-very-long"/>
        <relationship type="one" related-entity-name="Agreement"/>
        <relationship type="one" title="AgreementItemType" related-entity-name="Enumeration">
            <key-map field-name="agreementItemTypeEnumId"/>
        </relationship>
    </entity>
    <entity entity-name="AgreementItemGeo" package-name="mantle.party.agreement">
        <field name="agreementId" type="id" is-pk="true"/>
        <field name="agreementItemSeqId" type="id" is-pk="true"/>
        <field name="geoId" type="id" is-pk="true"/>
        <relationship type="one" related-entity-name="AgreementItem">
            <key-map field-name="agreementId"/>
            <key-map field-name="agreementItemSeqId"/>
        </relationship>
        <relationship type="one" related-entity-name="Geo"/>
    </entity>
    <entity entity-name="AgreementItemParty" package-name="mantle.party.agreement">
        <field name="agreementId" type="id" is-pk="true"/>
        <field name="agreementItemSeqId" type="id" is-pk="true"/>
        <field name="partyId" type="id" is-pk="true"/>
        <field name="roleTypeId" type="id" is-pk="true"/>
        <relationship type="one" related-entity-name="AgreementItem">
            <key-map field-name="agreementId"/>
            <key-map field-name="agreementItemSeqId"/>
        </relationship>
        <relationship type="one" related-entity-name="Party"/>
        <relationship type="one" related-entity-name="RoleType"/>
    </entity>
    <entity entity-name="AgreementItemProduct" package-name="mantle.party.agreement">
        <field name="agreementId" type="id" is-pk="true"/>
        <field name="agreementItemSeqId" type="id" is-pk="true"/>
        <field name="productId" type="id" is-pk="true"/>
        <field name="minQuantity" type="number-decimal" is-pk="true"/>
        <field name="price" type="currency-precise"/>
        <relationship type="one" related-entity-name="AgreementItem">
            <key-map field-name="agreementId"/>
            <key-map field-name="agreementItemSeqId"/>
        </relationship>
        <relationship type="one" related-entity-name="Product"/>
    </entity>
    <entity entity-name="AgreementItemWorkEffort" package-name="mantle.party.agreement">
        <field name="agreementId" type="id" is-pk="true"/>
        <field name="agreementItemSeqId" type="id" is-pk="true"/>
        <field name="workEffortId" type="id" is-pk="true"/>
        <relationship type="one" related-entity-name="AgreementItem">
            <key-map field-name="agreementId"/>
            <key-map field-name="agreementItemSeqId"/>
        </relationship>
        <relationship type="one" related-entity-name="WorkEffort"/>
    </entity>
    <entity entity-name="AgreementItemEmployment" package-name="mantle.party.agreement">
        <field name="agreementId" type="id" is-pk="true"/>
        <field name="agreementItemSeqId" type="id" is-pk="true"/>
        <field name="employmentId" type="id" is-pk="true"/>
        <relationship type="one" related-entity-name="Employment"/>
        <relationship type="one" related-entity-name="AgreementItem">
            <key-map field-name="agreementId"/>
            <key-map field-name="agreementItemSeqId"/>
        </relationship>
    </entity>

    <!-- ========================================================= -->
    <!-- mantle.party.communication -->
    <!-- ========================================================= -->

    <entity entity-name="CommunicationEvent" package-name="mantle.party.communication">
        <field name="communicationEventId" type="id" is-pk="true"/>
        <field name="communicationEventTypeId" type="id"/>
        <field name="contactMechTypeEnumId" type="id"/>
        <field name="statusId" type="id"/>

        <field name="originalCommEventId" type="id"/>
        <field name="parentCommEventId" type="id"/>

        <field name="fromContactMechId" type="id"/>
        <field name="toContactMechId" type="id"/>
        <field name="fromPartyId" type="id"/>
        <field name="fromRoleTypeId" type="id"/>
        <field name="toPartyId" type="id"/>
        <field name="toRoleTypeId" type="id"/>

        <field name="entryDate" type="date-time"/>
        <field name="datetimeStarted" type="date-time"/>
        <field name="datetimeEnded" type="date-time"/>

        <field name="subject" type="text-long"/>
        <field name="contentType" type="text-medium"/>
        <field name="content" type="text-very-long"/>
        <field name="note" type="text-long"/>

        <field name="reasonEnumId" type="id"/>
        <field name="contactListId" type="id"/>
        <field name="emailMessageId" type="id"/>

        <relationship type="one" related-entity-name="CommunicationEventType"/>
        <relationship type="one" title="ContactMechType" related-entity-name="Enumeration">
            <key-map field-name="contactMechTypeEnumId"/>
        </relationship>
        <relationship type="one" title="CommunicationEvent" related-entity-name="StatusItem"/>
        <relationship type="one" title="From" related-entity-name="ContactMech">
            <key-map field-name="fromContactMechId"/>
        </relationship>
        <relationship type="one" title="To" related-entity-name="ContactMech">
            <key-map field-name="toContactMechId"/>
        </relationship>
        <relationship type="one" title="From" related-entity-name="Party">
            <key-map field-name="fromPartyId"/>
        </relationship>
        <relationship type="one" title="From" related-entity-name="RoleType">
            <key-map field-name="fromRoleTypeId"/>
        </relationship>
        <relationship type="one" title="To" related-entity-name="Party">
            <key-map field-name="toPartyId"/>
        </relationship>
        <relationship type="one" title="To" related-entity-name="RoleType">
            <key-map field-name="toRoleTypeId"/>
        </relationship>
        <relationship type="one" related-entity-name="ContactList"/>
        <relationship type="one" title="Reason" related-entity-name="Enumeration">
            <key-map field-name="reasonEnumId"/>
        </relationship>
    </entity>
    <entity entity-name="CommunicationEventContent" package-name="mantle.party.communication">
        <field name="communicationEventId" type="id" is-pk="true"/>
        <field name="contentLocation" type="text-long"/>
        <field name="fromDate" type="date-time" is-pk="true"/>
        <field name="thruDate" type="date-time"/>
        <field name="assocTypeEnumId" type="id"/>
        <field name="sequenceNum" type="number-integer"/>
        <relationship type="one" related-entity-name="CommunicationEvent"/>
        <relationship type="one" title="AssocType" related-entity-name="Enumeration">
            <key-map field-name="assocTypeEnumId"/>
        </relationship>
    </entity>
    <entity entity-name="CommunicationEventProduct" package-name="mantle.party.communication">
        <field name="productId" type="id" is-pk="true"/>
        <field name="communicationEventId" type="id" is-pk="true"/>
        <relationship type="one" related-entity-name="Product"/>
        <relationship type="one" related-entity-name="CommunicationEvent"/>
    </entity>
    <entity entity-name="CommunicationEventPurpose" package-name="mantle.party.communication">
        <field name="communicationEventId" type="id" is-pk="true"/>
        <field name="purposeEnumId" type="id" is-pk="true"/>
        <field name="description" type="text-long"/>
        <relationship type="one" related-entity-name="CommunicationEvent"/>
        <relationship type="one" title="CommunicationPurpose" related-entity-name="Enumeration">
            <key-map field-name="purposeEnumId"/>
        </relationship>
    </entity>
    <entity entity-name="CommunicationEventParty" package-name="mantle.party.communication">
        <field name="communicationEventId" type="id" is-pk="true"/>
        <field name="partyId" type="id" is-pk="true"/>
        <field name="roleTypeId" type="id" is-pk="true"/>
        <field name="contactMechId" type="id"/>
        <field name="statusId" type="id"/>
        <relationship type="one" related-entity-name="CommunicationEvent"/>
        <relationship type="one" related-entity-name="Party"/>
        <relationship type="one" related-entity-name="RoleType"/>
        <relationship type="one" related-entity-name="ContactMech"/>
        <relationship type="one" title="CommunicationEventParty" related-entity-name="StatusItem"/>
    </entity>
    <entity entity-name="CommunicationEventType" package-name="mantle.party.communication">
        <field name="communicationEventTypeId" type="id" is-pk="true"/>
        <field name="parentTypeId" type="id"/>
        <field name="description" type="text-long"/>
        <field name="contactMechTypeEnumId" type="id"/>
        <relationship type="one" title="Parent" related-entity-name="CommunicationEventType">
            <key-map field-name="parentTypeId"/>
        </relationship>
        <relationship type="one" title="ContactMechType" related-entity-name="Enumeration">
            <key-map field-name="contactMechTypeEnumId"/>
        </relationship>
    </entity>

    <!-- ========================================================= -->
    <!-- mantle.party.contact -->
    <!-- ========================================================= -->

    <entity entity-name="ContactMech" package-name="mantle.party.contact">
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="contactMechTypeEnumId" type="id"/>
        <field name="infoString" type="text-long"/>
        <relationship type="one" title="ContactMechType" related-entity-name="Enumeration">
            <key-map field-name="contactMechTypeEnumId"/>
        </relationship>
        <index name="INFO_STRING_IDX"><index-field name="infoString"/></index>
    </entity>
    <entity entity-name="ContactMechPurpose" package-name="mantle.party.contact">
        <field name="contactMechPurposeId" type="id" is-pk="true"/>
        <field name="contactMechTypeEnumId" type="id"/>
        <field name="description" type="text-long"/>
        <relationship type="one" title="ContactMechType" related-entity-name="Enumeration">
            <key-map field-name="contactMechTypeEnumId"/>
        </relationship>
        <index name="INFO_STRING_IDX"><index-field name="infoString"/></index>
    </entity>
    <entity entity-name="EmailAddressVerification" package-name="mantle.party.contact">
        <description>Holds hashes for email address verification</description>
        <field name="emailAddress" type="id-very-long" is-pk="true"/>
        <field name="verifyHash" type="text-long"/>
        <field name="expireDate" type="date-time"/>
        <index name="EMAIL_VERIFY_HASH" unique="true"><index-field name="verifyHash"/></index>
    </entity>
    <entity entity-name="PartyContactMech" package-name="mantle.party.contact">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="contactMechPurposeId" type="id" is-pk="true"/>
        <field name="fromDate" type="date-time" is-pk="true"/>
        <field name="thruDate" type="date-time"/>
        <field name="roleTypeId" type="id"/>
        <field name="allowSolicitation" type="text-indicator"/>
        <field name="extension" type="text-long"/>
        <field name="verified" type="text-indicator"/>
        <field name="comments" type="text-long"/>
        <field name="validSince" type="date"/>
        <relationship type="one" related-entity-name="Party"/>
        <relationship type="one" related-entity-name="RoleType"/>
        <relationship type="one" related-entity-name="ContactMech"/>
        <relationship type="one" related-entity-name="ContactMechPurpose"/>
    </entity>
    <entity entity-name="PostalAddress" package-name="mantle.party.contact">
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="toName" type="text-medium"/>
        <field name="attnName" type="text-medium"/>
        <field name="address1" type="text-long"/>
        <field name="address2" type="text-long"/>
        <field name="unitNumber" type="text-medium"/>
        <field name="directions" type="text-long"/>
        <field name="city" type="text-medium"/>
        <field name="postalCode" type="text-short"/>
        <field name="postalCodeExt" type="text-short"/>
        <field name="countryGeoId" type="id"/>
        <field name="stateProvinceGeoId" type="id"/>
        <field name="countyGeoId" type="id"/>
        <field name="postalCodeGeoId" type="id"/>
        <field name="geoPointId" type="id"/>
        <relationship type="one" related-entity-name="ContactMech"/>
        <relationship type="one" title="Country" related-entity-name="Geo">
            <key-map field-name="countryGeoId"/>
        </relationship>
        <relationship type="one" title="StateProvince" related-entity-name="Geo">
            <key-map field-name="stateProvinceGeoId"/>
        </relationship>
        <relationship type="one" title="County" related-entity-name="Geo">
            <key-map field-name="countyGeoId"/>
        </relationship>
        <relationship type="one" title="PostalCode" related-entity-name="Geo">
            <key-map field-name="postalCodeGeoId"/>
        </relationship>
        <relationship type="one" related-entity-name="GeoPoint"/>
        <index name="CITY_IDX"><index-field name="city"/></index>
        <index name="POSTAL_CODE_IDX"><index-field name="postalCode"/></index>
    </entity>
    <entity entity-name="TelecomNumber" package-name="mantle.party.contact">
        <field name="contactMechId" type="id" is-pk="true"/>
        <field name="countryCode" type="text-short"/>
        <field name="areaCode" type="text-short"/>
        <field name="contactNumber" type="text-short"/>
        <field name="askForName" type="text-medium"/>
        <relationship type="one" related-entity-name="ContactMech"/>
        <index name="AREA_CONTACT_IDX"><index-field name="areaCode"/><index-field name="contactNumber"/></index>
    </entity>

    <!-- ========================================================= -->
    <!-- mantle.party.party -->
    <!-- ========================================================= -->

    <entity entity-name="Party" package-name="mantle.party.party">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="partyTypeEnumId" type="id"/>
        <field name="disabled" type="text-indicator" enable-audit-log="true"/>
        <field name="externalId" type="id"/>
        <field name="dataSourceId" type="id"/>
        <relationship type="one" title="PartyType" related-entity-name="Enumeration">
            <key-map field-name="partyTypeEnumId"/>
        </relationship>
        <relationship type="one" related-entity-name="StatusItem"/>
        <relationship type="one" related-entity-name="DataSource"/>
        <index name="PARTY_ID_EXT"><index-field name="externalId"/></index>
    </entity>
    <entity entity-name="Organization" package-name="mantle.party.party">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="organizationName" type="text-medium"/>
        <field name="officeSiteName" type="text-medium"/>
        <field name="annualRevenue" type="currency-amount"/>
        <field name="numEmployees" type="number-integer"/>
        <field name="comments" type="text-long"/>
        <relationship type="one" related-entity-name="Party"/>
        <index name="PTY_ORG_NAME_IDX"><index-field name="organizationName"/></index>
    </entity>
    <entity entity-name="Person" package-name="mantle.party.party">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="salutation" type="text-medium"/>
        <field name="firstName" type="text-medium"/>
        <field name="middleName" type="text-medium"/>
        <field name="lastName" type="text-medium"/>
        <field name="personalTitle" type="text-medium"/>
        <field name="suffix" type="text-medium"/>
        <field name="nickname" type="text-medium"/>
        <field name="gender" type="text-indicator"/>
        <field name="birthDate" type="date"/>
        <field name="deceasedDate" type="date"/>
        <field name="height" type="number-float"/>
        <field name="weight" type="number-float"/>
        <field name="mothersMaidenName" type="text-long" encrypt="true"/>
        <field name="maritalStatus" type="text-indicator"/>
        <field name="comments" type="text-long"/>
        <field name="employmentStatusEnumId" type="id"/>
        <field name="residenceStatusEnumId" type="id"/>
        <field name="occupation" type="text-medium"/>
        <relationship type="one" related-entity-name="Party"/>
        <relationship type="one" title="EmploymentStatus" related-entity-name="Enumeration">
            <key-map field-name="employmentStatusEnumId"/>
        </relationship>
        <relationship type="one" title="ResidenceStatus" related-entity-name="Enumeration">
            <key-map field-name="residenceStatusEnumId"/>
        </relationship>
        <index name="FIRST_NAME_IDX"><index-field name="firstName"/></index>
        <index name="LAST_NAME_IDX"><index-field name="lastName"/></index>
    </entity>

    <entity entity-name="PartyIdentification" package-name="mantle.party.party">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="partyIdTypeEnumId" type="id" is-pk="true"/>
        <field name="idValue" type="id-long"/>
        <field name="expireDate" type="date"/>
        <relationship type="one" title="PartyIdType" related-entity-name="Enumeration">
            <key-map field-name="partyIdTypeEnumId"/>
        </relationship>
        <relationship type="one" related-entity-name="Party"/>
        <index name="PARTY_ID_VALUE" unique="false"><index-field name="idValue"/></index>
    </entity>
    <entity entity-name="PartyGeoPoint" package-name="mantle.party.party">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="geoPointId" type="id" is-pk="true"/>
        <field name="fromDate" type="date-time" is-pk="true"/>
        <field name="thruDate" type="date-time"/>
        <relationship type="one" related-entity-name="Party"/>
        <relationship type="one" related-entity-name="GeoPoint"/>
    </entity>

    <entity entity-name="PartyClassification" package-name="mantle.party.party">
        <field name="partyClassificationId" type="id" is-pk="true"/>
        <field name="classificationTypeEnumId" type="id"/>
        <field name="parentClassificationId" type="id"/>
        <field name="description" type="text-long"/>
        <relationship type="one" title="PartyClassificationType" related-entity-name="Enumeration">
            <key-map field-name="classificationTypeEnumId"/>
        </relationship>
        <relationship type="one" title="Parent" related-entity-name="PartyClassification">
            <key-map field-name="parentClassificationId"/>
        </relationship>
    </entity>
    <entity entity-name="PartyClassificationAppl" package-name="mantle.party.party">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="partyClassificationId" type="id" is-pk="true"/>
        <field name="fromDate" type="date-time" is-pk="true"/>
        <field name="thruDate" type="date-time"/>
        <relationship type="one" related-entity-name="Party"/>
        <relationship type="one" related-entity-name="PartyClassification"/>
    </entity>

    <entity entity-name="PartyContent" package-name="mantle.party.party">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="contentLocation" type="text-long" is-pk="true"/>
        <field name="partyContentTypeEnumId" type="id" is-pk="true"/>
        <field name="fromDate" type="date-time" is-pk="true"/>
        <field name="thruDate" type="date-time"/>
        <relationship type="one" related-entity-name="Party"/>
        <relationship type="one" title="PartyContentType" related-entity-name="Enumeration">
            <key-map field-name="partyContentTypeEnumId"/>
        </relationship>
    </entity>
    <entity entity-name="PartyNote" package-name="mantle.party.party">
        <field name="partyId" type="id" is-pk="true"/>
        <field name="noteDate" type="date-time" is-pk="true"/>
        <field name="noteText" type="text-very-long"/>
        <relationship type="one" related-entity-name="Party"/>
    </entity>
    <entity entity-name="PartyRelationship" package-name="mantle.party.party">
        <field name="partyRelationshipId" type="id" is-pk="true"/>
        <field name="relationshipTypeEnumId" type="id"/>
        <field name="fromPartyId" type="id"/>
        <field name="fromRoleTypeId" type="id"/>
        <field name="toPartyId" type="id"/>
        <field name="toRoleTypeId" type="id"/>
        <field name="fromDate" type="date-time"/>
        <field name="thruDate" type="date-time"/>
        <field name="statusId" type="id"/>
        <field name="priorityTypeEnumId" type="id"/>
        <field name="permissionsEnumId" type="id"/>
        <field name="comments" type="text-long"/>
        <field name="relationshipName" type="text-medium">
            <description>Official name of relationship, such as title in an organization.</description>
        </field>
        <relationship type="one" title="From" related-entity-name="Party">
            <key-map field-name="fromPartyId"/>
        </relationship>
        <relationship type="one" title="From" related-entity-name="RoleType">
            <key-map field-name="fromRoleTypeId"/>
        </relationship>
        <relationship type="one" title="To" related-entity-name="Party">
            <key-map field-name="toPartyId"/>
        </relationship>
        <relationship type="one" title="To" related-entity-name="RoleType">
            <key-map field-name="toRoleTypeId"/>
        </relationship>
        <relationship type="one" title="PartyRelationship" related-entity-name="StatusItem"/>
