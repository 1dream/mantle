            <moqui.basic.Enumeration enumId="CREATED_MEDIA" description="Created Media" enumTypeId="WorkEffortContentType"/>
        </seed-data>
    </entity>
    <entity entity-name="WorkEffortDeliverableProd" package-name="mantle.work.effort">
